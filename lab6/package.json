{
   "name": "lab6",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "module",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/express": "^4.17.17",
      "body-parser": "^1.19.1",
      "cors": "^2.8.5",
      "express": "^5.0.0-beta.1",
      "pg": "^8.11.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.16",
      "winston": "^3.9.0",
      "yup": "^1.2.0"
   },
   "scripts": {
      "start": "node --loader ts-node/esm src/index.ts",
      "build": "tsc",
      "migration:generate": "typeorm-ts-node-esm migration:generate -d src/config/data-source.ts",
      "migration:run": "typeorm-ts-node-esm migration:run -d src/config/data-source.ts",
      "migration:revert": "npx typeorm-ts-node-esm migration:revert -d src/config/data-source.ts",
      "typeorm": "typeorm-ts-node-esm"
   }
}
